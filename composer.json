{
    "name": "ruslanakhatv/yii2-wildberries-sdk",
    "description": "Yii2 module for Wildberries PHP SDK integration",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "wildberries", "api", "sdk"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ruslanakhatv/yii2-wildberries-sdk/issues",
        "source": "https://github.com/ruslanakhatv/yii2-wildberries-sdk"
    },
    "authors": [
        {
            "name": "Ruslan Akhatov",
            "email": "ruslan.akhatov@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.0",
        "ruslanakhatv/wb-php-sdk": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "wb\\sdk\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "wb\\sdk\\Bootstrap"
    }
}
